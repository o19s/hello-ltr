        "store": true
      },
      "text_people_notf": {
        "type": "text",
        "analyzer": "names",
        "similarity": "no_tf_similarity"
      },
      "text_people_notf_front": {
        "type": "text",
        "analyzer": "names_front",
        "similarity": "no_tf_similarity"
      }
    }
  },
  "settings": {
    "analysis": {
      "filter": {
        "my_length_limit": {
          "type": "limit",
          "max_token_count": 10
        },
        "my_bigram_filter": {
          "type": "shingle",
          "max_shingle_size": 2,
          "min_shingle_size": 2,
          "output_unigrams": false},
        "english_stop": {
          "type": "stop",
          "stopwords": "_english_"
        },
        "english_keywords": {
          "type": "keyword_marker",
          "keywords": []
        },
        "english_stem": {
          "type": "stemmer",
          "language": "english"
        },
        "english_possessive_stem": {
          "type": "stemmer",
          "language": "possessive_english"
        },
        "bigrams": {
          "type": "shingle",
          "max_shingle_size": 2,
          "min_shingle_size": 2,
          "output_unigrams": false
        },
        "syn": {
          "type": "synonym",
          "synonyms": [
            "wife => bride",
            "wife,spouse"
          ]
        },
        "syn_bidirect": {
          "type": "synonym",
          "synonyms": [
            "wife,bride",
            "wife,spouse"
          ]
        },
        "syn_directed": {
          "type": "synonym",
          "synonyms": [
            "wife => wife, bride",
            "spouse => spouse, husband, wife, partner",
            "tunes => cartoons, toons, songs"
          ]
        },
        "syn_multiterm": {
          "type": "synonym",
          "synonyms": [
            "looney tunes, cartoons",
            "science fiction, sci fi, sci-fi, scifi"
          ]
        },
        "syn_gr_multiterm": {
          "type": "synonym_graph",
          "synonyms": [
            "looney tunes, cartoons",
            "science fiction, sci fi, sci-fi, scifi"
          ]
        },
        "syn_idioms": {
          "type": "synonym",
          "synonyms": [
            "looneytunes, looney tunes, looney toons => looney_tunes",
            "sci fi, scifi, science fiction => science_fiction"
          ]
        },
        "semantic_idioms": {
          "type": "synonym",
          "synonyms": [
            "sci fi, scifi, science fiction => science_fiction",
            "fantasy => fantasy"
          ]
        },
        "semantic_keep": {
          "type": "keep",
          "keep_words": ["science_fiction", "fantasy", "nerddom", "fanfic",
                         "__science__", "__fiction__",
                         "__nerd__", "__dom__",
                         "__fant__", "__asy__",
                         "__fan__", "__fic__"]
        },
        "semantic_expansion": {
          "type": "synonym",
          "synonyms": [
            "science_fiction => science_fiction, nerddom",
            "fantasy => fantasy"
          ]
        },
        "semantic_expansion_gr": {
          "type": "synonym_graph",
          "synonyms": [
            "science_fiction => __science__ __fiction__, __nerd__ __dom__",
            "fantasy => __fant__ __asy__,  __nerd__ __dom__",
            "fanfic => __fan__ __fic__, __nerd__ __dom__"
          ]
        },
        "semantic_expansion_sausage": {
          "type": "synonym",
          "synonyms": [
            "science_fiction => __science__ __fiction__, __nerd__ __dom__",
            "fantasy => __fant__ __asy__, __nerd__ __dom__",
            "fanfic => __fan__ __fic__, __nerd__ __dom__"
          ]
        }
          
      },
      "analyzer": {
        "names": {
          "type": "custom",
          "tokenizer": "standard",
          "filter": [
            "lowercase",
            "english_possessive_stem"
          ]
        },
        "names_bigram": {
          "type": "custom",
          "tokenizer": "standard",
          "filter": [
            "lowercase",
            "english_possessive_stem",
            "my_bigram_filter"
          ]
        },
        "names_front": {
          "type": "custom",
          "tokenizer": "standard",
          "filter": [
            "lowercase",
            "english_possessive_stem",
            "my_length_limit"
          ]
        },
        "my_wdf": {
          "type": "custom",
          "tokenizer": "standard",
          "filter": [
              "word_delimiter",
              "lowercase"
          ]
        },
        "my_keyword": {
          "type": "custom",
          "tokenizer": "keyword",
          "filter": [
              "lowercase"
          ]
        },
        "my_english": {
          "type": "custom",
          "char_filter": [
            "html_strip"
          ],
          "tokenizer": "standard",
          "filter": [
            "lowercase",
            "english_possessive_stem",
            "syn_directed",
            "english_stop",
            "english_keywords",
            "english_stem"
          ]
        },
        "std_tax": {
          "type": "custom",
          "tokenizer": "standard",
          "filter": [
            "lowercase",
            "semantic_idioms",
            "semantic_keep",
            "semantic_expansion"
          ]
        },
        "std_tax_phrase_q": {
          "type": "custom",
          "tokenizer": "standard",
          "filter": [
            "lowercase",
            "semantic_idioms",
            "semantic_keep",
            "semantic_expansion_gr"
          ]
        },
        "std_tax_phrase_idx": {
          "type": "custom",
          "tokenizer": "standard",
          "filter": [
            "lowercase",
            "semantic_idioms",
            "semantic_keep",
            "semantic_expansion_sausage"
          ]
        },
        "std_syn": {
          "type": "custom",
          "tokenizer": "standard",
          "filter": [
            "lowercase",
            "syn"
          ]
        },
        "std_syn_bidirect": {
          "type": "custom",
          "tokenizer": "standard",
          "filter": [
            "lowercase",
            "syn_bidirect"
          ]
        },
        "std_syn_directed": {
          "type": "custom",
          "tokenizer": "standard",
          "filter": [
            "lowercase",
            "syn_directed"
          ]
        },
        "std_syn_multiterm": {
          "type": "custom",
          "tokenizer": "standard",
          "filter": [
            "lowercase",
            "syn_multiterm"
          ]
        },
        "std_syn_gr_multiterm": {
          "type": "custom",
          "tokenizer": "standard",
          "filter": [
            "lowercase",
            "syn_gr_multiterm"
          ]
        },
        "std_idioms": {
          "type": "custom",
          "tokenizer": "standard",
          "filter": [
            "lowercase",
            "syn_idioms"
          ]
        },
        "std_syn_bigram_graph": {
          "type": "custom",
          "tokenizer": "standard",
          "filter": [
            "lowercase",
            "syn_gr_multiterm",
            "bigrams"
          ]
        }
      }
    },
    "index": {
      "similarity": {
        "no_tf_similarity": {
          "type": "BM25",
          "k1": 0.0,
          "b": 1.0
        }
      },
      "number_of_shards": 1
    }
  }
}
