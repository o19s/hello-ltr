      },
      "text_people": {
        "type": "text",
        "analyzer": "my_english",
        "fielddata": true,
        "store": true
      },
      "text_people_bigram": {
        "type": "text",
        "analyzer": "names_bigram",
        "fielddata": true,
        "store": true,
        "similarity": "no_tf_similarity"
      },
      "text_std": {
        "type": "text",
        "analyzer": "standard",
        "search_analyzer": "std_syn",
        "store": true
      },
      "text_std_idioms": {
        "type": "text",
        "analyzer": "std_idioms",
        "store": true
      },
      "text_std_tax": {
        "type": "text",
        "analyzer": "std_tax",
        "store": true
      },
      "text_std_tax_phrase": {
        "type": "text",
        "analyzer": "std_tax_phrase_idx",
        "search_analyzer": "std_tax_phrase_q",
        "store": true
      },
      "text_std_gr_idx_syn": {
        "type": "text",
        "analyzer": "std_syn_gr_multiterm",
