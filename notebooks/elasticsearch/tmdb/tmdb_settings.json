          "text_std",
          "text_all",
          "text_std_gr_idx_syn",
          "text_std_syn",
          "text_std_idioms",
          "text_std_tax",
          "text_std_tax_phrase"
        ],
        "fielddata": true
      },
      "text_all": {
        "type": "text",
